//whether positive or negative number

public class Pn {
	public static void main(String[] args) {
		int a=10;
        System.out.println("input number:"+a);
		if (a>0)
			System.out.println("Number is Positive");
		else if(a<0)
			System.out.println("Number is Negative");
		else
			System.out.println("Number is Zero");

	}

}
ouput:
input number:10
Number is Positive



//greatest among 3 number
public class greater3 {
	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		int c = 15;
		System.out.println("Input the number:"+a);
		System.out.println("Input the number:"+b);
		System.out.println("Input the number:"+c);
		if (a >= b && a >= c)
			System.out.println("The Greatest:"+a);
		else if (b >= a && b >= c)
			System.out.println("The Greatest:"+b);
		else
			System.out.println("The Greatest:"+c);

	}

}
output:
Input the number:10
Input the number:20
Input the number:15
The Greatest:20




//display the name of the week
public class weeks {
	public static void main(String[] args) {
		int a=7;
		switch (a) {
		case 1:
			System.out.println("monday");
			break;
		case 2:
			System.out.println("tuesday");
			break;
		case 3:
			System.out.println("wednesday");
			break;
		case 4:
			System.out.println("thursday");
			break;
		case 5:
			System.out.println("friday");
			break;
		case 6:
			System.out.println("saturday");
			break;
		case 7:
			System.out.println("sunday");
			break;
		default:
			System.out.println("none");
		}
	}

}
output:
Input number:7
sunday






